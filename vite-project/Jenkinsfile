pipeline {
    agent any

    tools {
        nodejs "react"
    }

    stages {
        stage('checking version') {
            steps {
               bat 'npm -v'
               bat 'node -v'
            }
        }
        stage('Building') {
            steps {   
             bat '''
             cd vite-project
             npm run build
            
             '''
            }
        }
          stage('runing') {
            steps {   
             bat '''
             cd vite-project
             npm run dev
             '''
            }
        }
    }
}